{
  "name": "webpack-app",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --inline --hot --port 777 --history-api-fallback",
    "build": "webpack --env production",
    "serv": "static build",
    "lint": "eslint --quiet --ext .js sources",
    "lint:fix": "eslint --quiet --fix --ext .js sources"
  },
  "devDependencies": {
    "antd": "latest",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.22.0",
    "classnames": "^2.2.5",
    "color-functions": "^1.1.0",
    "create-react-class": "15.6.3",
    "css-loader": "^0.28.4",
    "file-loader": "^1.1.11",
    "js-tree": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.0.6",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loading": "0.0.9",
    "react-mixin": "4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-ui-tree": "^2.6.0",
    "redux": "^3.7.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.16.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "eslint": "3.13.0",
    "babel-eslint": "^7.1.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-defaults": "9.0.0",
    "eslint-config-google": "0.7.1",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-plugin-react-native": "2.2.1",
    "eslint-plugin-standard": "2.0.1"
  },
  "dependencies": {
    "antd": "latest",
    "autoprefixer": "^8.5.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "classnames": "^2.2.5",
    "color-functions": "^1.1.0",
    "create-react-class": "15.6.3",
    "js-tree": "^1.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loading": "0.0.9",
    "react-mixin": "4.0.0",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-ui-tree": "^2.6.0",
    "redux": "^3.7.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.16.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2015-node6",
      "react" 
    ],
    "plugins": [
      "transform-class-properties",
      "transform-es2015-destructuring",     
      "transform-object-assign",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react",
      "react-native"
    ],
    "extends": "airbnb",
    "rules": {
      "no-undef": 0,
      "import/extensions": 0,
      "import/prefer-default-export": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": [
        "error"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-bind": 0,
      "linebreak-style": 0,
      "react/no-string-refs": 1,
      "react/forbid-prop-types": 1,
      "no-prototype-builtins": 0
    }
  }
}
